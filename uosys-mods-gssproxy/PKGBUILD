# Maintainer: tinywrkb <tinywrkb@gmail.com>

_pkgname=gssproxy
pkgname=uosys-mods-${_pkgname}
pkgver=2021.01.26.1
pkgrel=1
pkgdesc="$_pkgname uosys mods"
arch=('any')
licesne=('GPL')
url=''

build() {
  cat << EOF > tmpfiles.d.conf
# $_pkgname
# TODO: test without this
d /var/lib/gssproxy - - - - -
d /var/lib/gssproxy/clients - - - - -
d /var/lib/gssproxy/rcache - - - - -
d /var/log/gssproxy - - - - -
EOF
}

package() {
  depends+=('uosys-mods' $_pkgname)
  source /usr/lib/os/os-mods-install
}
