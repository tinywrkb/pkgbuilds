_pkgname=mimeo
pkgname=uosys-mods-${_pkgname}
pkgver=2022.02.04.1
pkgrel=1
pkgdesc="$_pkgname uosys mods"
arch=('any')
license=('GPL')
url=''
depends=('uosys-mods')

package() {
  depends+=($_pkgname)
  install -dm755 ${pkgdir}/usr/bin
  ln -s mimeo ${pkgdir}/usr/bin/xdg-open
}
