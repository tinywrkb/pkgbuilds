_pkgname=qemu
pkgname=uosys-mods-${_pkgname}
pkgver=2021.01.26.1
pkgrel=1
pkgdesc="$_pkgname uosys mods"
arch=('any')
licesne=('GPL')
url=''
depends=('uosys-mods')
source=('rules')
sha256sums=('SKIP')

package() {
  depends+=($_pkgname)
  source /usr/lib/os/os-mods-install
  install -Dm644 rules ${pkgdir}/usr/lib/udev/rules.d/80-qemu_kvm.rules
}
