_pkgname=bash
pkgname=uosys-mods-${_pkgname}
pkgver=2022.01.25.1
pkgrel=1
pkgdesc="$_pkgname uosys mods"
arch=('any')
license=('GPL')
url=''
depends=('uosys-mods')
source=('patch' 'bash.functions')
sha256sums=('SKIP' 'SKIP')

package() {
  depends+=($_pkgname)
  source /usr/lib/os/os-mods-install
  install -Dm644 bash.functions -t ${pkgdir}/etc/
}
