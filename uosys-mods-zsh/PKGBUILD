# Maintainer: tinywrkb <tinywrkb@gmail.com>

_pkgname=zsh
pkgname=uosys-mods-${_pkgname}
pkgver=2021.01.26.1
pkgrel=1
pkgdesc="$_pkgname uosys mods"
arch=('any')
licesne=('GPL')
url=''
source=('zshenv')
sha256sums=('SKIP')


package() {
  depends+=('uosys-mods' $_pkgname)
  source /usr/lib/os/os-mods-install
  install -Dm644 zshenv -t ${pkgdir}/etc/zsh/
}
